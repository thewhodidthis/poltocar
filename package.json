{
  "name": "poltocar",
  "version": "1.0.3",
  "description": "polar to x, y",
  "keywords": [
    "car",
    "2",
    "pol"
  ],
  "jsnext:main": "index.es",
  "main": "index.js",
  "homepage": "https://github.com/thewhodidthis/poltocar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/poltocar.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/poltocar/issues"
  },
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "node test",
    "clear": "npm run clear:dist && npm run clear:logs",
    "clear:dist": "rm -rf dist/",
    "clear:logs": "rm -f *.log",
    "lint": "eslint index.es; exit 0",
    "start": "npm run watch",
    "watch": "rollup -c -w",
    "preexample": "npm run build",
    "example": "node example",
    "example:link": "ln -siv \"$(pwd)/dist/poltocar.min.js\" example/poltocar.js",
    "example:push": "rsync -avruzLPC --exclude=.DS_Store ./example/ $npm_package_config_remote",
    "example:serve": "php -S localhost:8000 -t example/",
    "prebuild": "mkdir -p dist/",
    "build": "rollup -c",
    "build:min": "uglifyjs dist/poltocar.js -cm > dist/poltocar.min.js",
    "postbuild": "npm run build:min",
    "preversion": "npm run clear && npm run build"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-preset-es2015": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-config-airbnb-base": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-watch": "latest",
    "tape": "latest",
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
