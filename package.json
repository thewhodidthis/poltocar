{
  "name": "poltocar",
  "version": "0.0.0",
  "description": "polar to x, y",
  "keywords": [
    "",
    "",
    ""
  ],
  "main": "index.js",
  "homepage": "https://github.com/thewhodidthis/poltocar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/poltocar.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/poltocar/issues"
  },
  "directories": {
    "example": "example"
  },
  "config": {
    "jscsrc": ".jscsrc"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "npm run clear:dist && npm run clear:logs",
    "clear:dist": "rm -rf dist/",
    "clear:logs": "rm -f *.log",
    "lint": "(jshint index.js & jscs index.js --reporter=inlinesingle) 2>&1 | tee lint-js.log",
    "postlint": "find lint-js.log -size 0 -print0 | xargs -0 rm",
    "start": "npm run watch",
    "watch": "nodemon -e js -w index.js -x 'npm run build'",
    "preexample": "npm run build",
    "example": "node example",
    "prebuild": "mkdir -p dist/",
    "build": "browserify index.js -s poltocar -o ./dist/poltocar.js",
    "build:min": "browserify index.js -s poltocar | uglifyjs -mc -o ./dist/poltocar.min.js",
    "postbuild": "npm run build:min",
    "preversion": "npm run clear && npm run build"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "jscs": "latest",
    "jshint": "latest",
    "uglify-js": "latest",
    "browserify": "latest",
    "nodemon": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}

